<%= form_tag({controller: 'sessions', action: 'create', method: 'post'}) do %>
<%= label_tag :user_name %>
<%= select_tag :user_name, options_from_collection_for_select( User.all, :name, :name) %><br>

<%= label_tag :user_password %>
<%= password_field_tag :user_password %><br>

<%= submit_tag "Sign In"%>
<% end %>
